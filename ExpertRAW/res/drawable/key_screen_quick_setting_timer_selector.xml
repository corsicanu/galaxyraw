<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:drawable="@drawable/ic_camera_quick_ic_timer_off" />
    <item android:state_selected="true">
        <level-list>
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_off_on" android:maxLevel="0" />
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_2_on" android:maxLevel="1" />
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_5_on" android:maxLevel="2" />
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_10_on" android:maxLevel="3" />
        </level-list>
    </item>
    <item>
        <level-list>
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_off" android:maxLevel="0" />
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_2" android:maxLevel="1" />
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_5" android:maxLevel="2" />
            <item android:drawable="@drawable/ic_camera_quick_ic_timer_10" android:maxLevel="3" />
        </level-list>
    </item>
</selector>