        </level-list>
    </item>
    <item>
        <level-list>
            <item android:drawable="@drawable/ic_camera_quick_ic_flash_off" android:maxLevel="0" />
            <item android:drawable="@drawable/ic_camera_quick_ic_flash_auto" android:maxLevel="1" />
            <item android:drawable="@drawable/ic_camera_quick_ic_flash_normal" android:maxLevel="2" />
        </level-list>
    </item>
</selector>